<table id="certificateTable" class="table table-striped" style="width:100%">
  <thead>
    <tr>
      {% for column in header %}
        <th>{{ column }}</th>
      {% endfor %}
    </tr>
  </thead>
  <tbody>
    {% for row in rows %}
      <tr>
        <td>{{ row.id }}</td>
        <td>{{ row.name }}</td>
        <td>{{ row.code }}</td>
        <td>{{ row.url }}</td>
      </tr>
    {% endfor %}
  </tbody>
  <tfoot>
    <tr>
      {% for column in header %}
        <th>{{ column }}</th>
      {% endfor %}
    </tr>
  </tfoot>
</table>
